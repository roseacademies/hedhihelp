    <!--The following strings correspond to the modules-->
    <string name="module_1_1">Anafurahi kwenda shule.</string>
    <string name="module_1_2">Unapoendelea, atakua na kubadili kama wasichana wote duniani wanavyofanya.</string>
    <string name="module_1_3">Ataongezeka kwa urefu kila mwaka.</string>
    <string name="module_1_4">Ataona mabadiliko zaidi wakati akikua na kuwa mwanamke.</string>
    <string name="module_1_5">Ataendeleza kifua. Ni kawaida kwa kifua kimoja kuwa kikubwa zaidi kuliko kingine.</string>
    <string name="module_1_6">Anaweza kuhitaji nguo mpya.</string>
    <string name="module_1_7">Yake makalio utapata pana.</string>
    <string name="module_1_8">Mabadiliko haya yote hufanya mwili wake uwe tayari kuwa wanawake, na kwa ajili ya hedhi.</string>
    <string name="module_1_9">Marafiki zake wote pia hubadilisha, baadhi ya haraka, polepole. </string>
    <string name="module_1_10">Kubadilisha ni nzuri. Watu hukua tofauti na hii ni ya kawaida.</string>

